meta {
  name: List all API Keys
  type: http
  seq: 5
}

get {
  url: {{base_url}}/{{version}}/account/integration/credentials/api-key
  body: none
  auth: inherit
}

script:post-response {
  bru.setVar('credential_id', res.body?.credentials[0]?.credential_id || null);
}

tests {
  test("should be able to list API keys", () => {
    const data = res.getBody();
    expect(res.getStatus()).to.equal(200);
    expect(data.credentials).to.exist;
  });
  
}
