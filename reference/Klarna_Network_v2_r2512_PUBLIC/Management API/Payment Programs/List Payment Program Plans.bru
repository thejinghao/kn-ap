meta {
  name: List Payment Program Plans
  type: http
  seq: 1
}

get {
  url: {{base_url}}/{{version}}/distribution/products/payment/payment-program-plans
  body: none
  auth: inherit
}

script:post-response {
  bru.setVar('payment_program_plan_id', res.body?.payment_program_plans[0].payment_program_plan_id || null);
  bru.setVar('payment_program_plan_version', res.body?.payment_program_plans[0].versions[0].version || null);
}
