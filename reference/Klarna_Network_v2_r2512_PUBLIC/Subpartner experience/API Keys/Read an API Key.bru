meta {
  name: Read an API Key
  type: http
  seq: 2
}

get {
  url: {{base_url}}/{{version}}/account/integration/credentials/api-key/:credential_id
  body: none
  auth: inherit
}

params:path {
  credential_id: {{credential_id}}
}

tests {
  test("should be able to read an API key", () => {
    const data = res.getBody();
    expect(res.getStatus()).to.equal(200);
    expect(data.credential_id).to.match(/^krn:/)
  });
  
}
