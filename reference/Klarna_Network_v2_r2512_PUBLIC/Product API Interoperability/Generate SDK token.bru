meta {
  name: Generate SDK token
  type: http
  seq: 1
}

post {
  url: {{base_url}}/{{version}}/accounts/:partner_account_id/interoperability/sdk-tokens
  body: none
  auth: inherit
}

params:path {
  partner_account_id: {{partner_account_id}}
}

headers {
  Klarna-Interoperability-Token: {{interoperability_token}}
  ~Klarna-Network-Session-Token: {{klarna_network_session_token}}
}
